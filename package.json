{
  "name": "esnext-coverage-format-html",
  "version": "0.0.8",
  "description": "esnext-coverage output formatter to HTML",
  "main": "dist/index.js",
  "scripts": {
    "dev": "GULP_ENV=dev gulp build",
    "dev-live": "npm test && GULP_ENV=dev-live gulp serve",
    "dist": "NODE_ENV=production GULP_ENV=dist gulp build",
    "dist-live": "npm test && NODE_ENV=production GULP_ENV=dist-live gulp serve",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "mocha -r esnext-coverage -r jsdom-global/register test/*.spec.js",
    "prepublish": "npm run lint && npm run dist",
    "start": "npm run dev-live"
  },
  "author": "Oleg Sklyanchuk <sklyanchuk@gmail.com> (http://olegskl.com)",
  "license": "MIT",
  "dependencies": {
    "esnext-coverage": "^0.0.7",
    "esnext-coverage-analytics": "^0.0.5",
    "fuzzaldrin": "^2.1.0",
    "redux": "^3.6.0",
    "url-parse": "^1.1.3",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.15.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.4.0",
    "eslint-config-meetic": "^4.0.0",
    "eslint-plugin-mocha": "^4.5.1",
    "etag": "^1.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.2",
    "gulp-cssmin": "^0.1.7",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.1",
    "gulp-inject": "^4.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.9",
    "jsdom": "^9.12.0",
    "jsdom-global": "^2.1.1",
    "merge-stream": "^1.0.0",
    "mocha": "^3.0.2",
    "postcss-custom-properties": "^5.0.1",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^9.0.0",
    "run-sequence": "^1.2.2",
    "through2": "^2.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
